{
  "name": "pohi-gitlab-ci",
  "version": "0.1.0",
  "description": "PoHI Human Approval for GitLab CI/CD",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "pohi-gitlab-ci": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js --format=esm --banner:js='#!/usr/bin/env node'",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "pohi",
    "gitlab",
    "ci",
    "human-verification",
    "world-id"
  ],
  "author": "Ikko Ashimine <eltociear@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/pohi-protocol/pohi.git",
    "directory": "packages/gitlab-ci"
  },
  "devDependencies": {
    "esbuild": "^0.20.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  }
}
